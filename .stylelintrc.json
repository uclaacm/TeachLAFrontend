{
  "extends": "stylelint-config-sass-guidelines",
  "plugins": [
    "stylelint-scss"
  ],
  "ignoreFiles": ["**/*.ts", "**/*.tsx", "dist/*", "node_modules/*", "coverage/*"],
  "rules": {
    "color-named": null,
    "selector-max-id": null,
    "scss/at-import-partial-extension-blacklist": null
  }
}
